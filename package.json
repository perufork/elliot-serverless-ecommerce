{
	"name": "next-checkout-zeit",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"dev": "next",
		"server": "BASE_URL=http://localhost:3000 node --icu-data-dir=node_modules/full-icu server.js",
		"build": "next build",
		"extract": "node ./src/scripts/default-lang && node ./src/scripts/extract",
		"start": "NODE_ENV=production node --icu-data-dir=node_modules/full-icu server.js"
	},
	"dependencies": {
		"@formatjs/cli": "^1.1.15",
		"@formatjs/intl-relativetimeformat": "^4.5.4",
		"@formatjs/intl-utils": "^2.0.1",
		"@fortawesome/fontawesome-svg-core": "^1.2.26",
		"@fortawesome/free-regular-svg-icons": "^5.12.0",
		"@fortawesome/free-solid-svg-icons": "^5.12.0",
		"@fortawesome/react-fontawesome": "^0.1.8",
		"accepts": "^1.3.7",
		"atob": "^2.1.2",
		"axios": "^0.19.2",
		"btoa": "^1.2.1",
		"classnames": "^2.2.6",
		"convert-units": "^2.3.4",
		"country-currency-map": "^2.1.6",
		"detect-browser": "^4.8.0",
		"dotenv": "^8.2.0",
		"formik": "^2.0.8",
		"full-icu": "^1.3.0",
		"glob": "^7.1.6",
		"intl": "^1.2.5",
		"next": "9.1.6",
		"next-images": "^1.2.0",
		"react": "16.12.0",
		"react-dom": "16.12.0",
		"react-flex-ready": "^1.1.0",
		"react-hook-form": "^4.9.6",
		"react-id-swiper": "^2.4.0",
		"react-intl": "^3.9.3",
		"react-spring": "^8.0.27",
		"react-tabs": "^3.1.0",
		"styled-components": "^4.4.1",
		"swiper": "^5.2.1",
		"yup": "^0.28.0"
	},
	"devDependencies": {
		"babel-eslint": "^10.0.3",
		"babel-plugin-module-resolver": "^4.0.0",
		"babel-plugin-react-intl": "^5.1.13",
		"babel-plugin-styled-components": "^1.10.6",
		"eslint": "^6.8.0",
		"eslint-plugin-react": "^7.17.0",
		"husky": "^4.2.0",
		"lint-staged": "^10.0.2",
		"prettier": "^1.19.1"
	},
	"prettier": {
		"useTabs": true,
		"printWidth": 80,
		"semi": true
	},
	"eslintConfig": {
		"parser": "babel-eslint"
	},
	"eslintIgnore": [
		"**/node_modules/**/*",
		"**/.next/**/*"
	],
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"lint-staged": {
		"*.{js,css,json,md}": [
			"prettier --write"
		],
		"*.js": [
			"eslint --fix"
		]
	}
}
